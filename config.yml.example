# GP Kitchen Configuration

# Development mode - enables Update Prices button
dev_mode: true

# Database paths (relative to app directory)
database:
  main_path: "data/gp_kitchen.db"
  prices_path: "data/prices.db"

# Session secret - change in production
session:
  secret: "change_this_to_a_random_string_in_production"

# User agent for OSRS Wiki API (required by their API)
user_agent: "GP-Kitchen/1.0"
